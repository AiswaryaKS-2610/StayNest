{
  "devDependencies": {
    "cordova-android": "^14.0.1",
    "cordova-plugin-camera": "^8.0.0",
    "cordova-plugin-geolocation": "^5.0.0"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-camera": {
        "GPS_REQUIRED": "true",
        "ANDROIDX_CORE_VERSION": "1.6.+"
      }
    }
  },
  "dependencies": {
    "leaflet-geosearch": "^4.2.2"
  }
}
